{% schema %}
  {
    "name": "Naperee header",
    "class": "section-header naptec__header naperee__header-wrapper-bg",
    "max_blocks": 3,
    "settings": [
      {
        "id": "root-page",
        "label": "Page root for this header",
        "type": "page",
        "info": "if brand is naperee, select naperee page"
      },
      {
        "type": "color",
        "id": "header_background",
        "label": "Header background",
        "default": "#073b43"
      },
      {
