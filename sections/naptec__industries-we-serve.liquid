{{ "naptec__industries-we-serve.css" | asset_url | stylesheet_tag }}
<section class="page-width naptec__industries-we-serve color-scheme-1">
  <div class="naptec__industries-wrapper">
    <h2>
      {{ section.settings.title }}
    </h2>
    <ul class="naptec__industries-items">
      {% for block in section.blocks %}
        {% case block.type %}
          {% when 'industriy' %}
            <li>
              <p>{{ block.settings.industry_name }}</p>
            </li>
        {% endcase %}
      {% endfor %}
    </ul>
  </div>
</section>

{% schema %}
  {
    "name": "Industries we serve",
    "settings": [
      {
        "type": "text",
        "default": "Industries we serve",
        "id": "title",
        "label": "Section's title"
      }
    ],
    "blocks": [
      {
        "type": "industriy",
        "name": "Industry",
        "settings": [
          {
            "type": "text",
            "id": "industry_name",
            "label": "Industry name"
          }
        ]
      }
    ],
    "presets": [
      {
        "name": "Industries we serve"
      }
    ]
  }
{% endschema %}